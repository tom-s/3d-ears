  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "scripts": {
    "dev":  "npm run server:dev & npm run front:dev",
    "server:dev": "`npm bin`/nodemon --watch api/src/ api/src/server.js --exec 'npm run server:babel'",
    "server:babel": "babel-node",
    "front:build": "node_modules/.bin/webpack --config webpack/prod.config.js",
    "front:dev": "node_modules/.bin/webpack-dev-server --config webpack/dev.config.js",
    "test": "npm run lint && npm run mocha",
    "test:watch": "npm run lint && test -- --watch",
    "mocha": "node_modules/.bin/mocha --compilers js:babel-core/register app/tests/mocha/**/*Spec.js --recursive",
    "lint": "node_modules/.bin/eslint . --ext .js --ext .jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mezod/boilerplate-koa-redux-react.git"
  },
